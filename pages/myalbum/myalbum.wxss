/* pages/myalbum/myalbum.wxss */
@import "/utils/animate.wxss";

.flex {
    display: flex;
    height: 100vh;
    width: 100vw;
    justify-content: center;
    align-items: center;
    flex-wrap: wrap;
}

.lunbo{
  width: 100%;
  height: 80%;
}

.text {
    font-size: 90rpx;
    font-weight: lighter;
    background: linear-gradient(to right, #1ae8c8, #4a8aff);
    -webkit-background-clip: text;
    background-clip: text;
    color: transparent;
}


.btn{
  width: 100%;
  display: flex;
  flex-direction: row;
  position: fixed;
  bottom: 5px;
  border-radius: 10px;
}